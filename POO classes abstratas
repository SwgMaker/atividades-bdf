class Animal:
    def falar(self):
        raise NotImplementedError("A subclasse = falar()")

class Cachorro(Animal):
    def falar(self):
        return "Au Au!"

class Gato(Animal):
    def falar(self):
        return "Miau!"

meu_cachorro = Cachorro()
meu_gato = Gato()

print(f"O cachorro diz: {meu_cachorro.falar()}")
print(f"O gato diz: {meu_gato.falar()}")

animais = [Cachorro(), Gato(), Cachorro()]

print("zoologico:")
for animal in animais:
    print(animal.falar())